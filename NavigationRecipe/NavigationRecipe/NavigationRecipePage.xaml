<?xml version="1.0" encoding="utf-8"?>
<Shell xmlns="http://xamarin.com/schemas/2014/forms"
     xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
     xmlns:local="clr-namespace:NavigationRecipe"
     x:Class="NavigationRecipe.AppShell">
    <TabBar>
        <Tab Title="Tab 1">
            <ShellContent ContentTemplate="{DataTemplate local:MyFirstTabPage}" />
        </Tab>
        <Tab Title="Tab 2">
            <ShellContent ContentTemplate="{DataTemplate local:MySecondTabPage}" />
        </Tab>
    </TabBar>
</Shell>